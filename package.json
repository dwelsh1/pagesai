{
  "name": "pagesai",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "tsc": "tsc --noEmit",
    "type-check": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "test:unit": "vitest run --config vitest.config.mjs",
    "test:watch": "vitest --watch --config vitest.config.mjs",
    "test:coverage": "vitest run --coverage --config vitest.config.mjs",
    "test:ui": "vitest --ui --config vitest.config.mjs",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "check-all": "npm run type-check && npm run lint && npm run format && npm run test:unit"
  },
